{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "require-jsdoc": 0,
        "max-len": 0,
        "object-curly-spacing": ["error", "always"]
    }
}
